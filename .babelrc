{
    "presets": [
      [
        "env", {
          "targets": {
            "browsers": [
              "last 5 versions",
              "Android >= 4.0", 
              "iOS >= 8",
              "safari > 8",
              "not ie <= 8"
            ]
          },
          "modules": false,
          "useBuiltIns": "entry",
          "loose": true
        }
      ],
      "stage-2"
    ],
    "plugins": ["transform-async-to-promises"]
}